<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Version>1.0.8</Version>
        <PackageProjectUrl>https://github.com/D-Diyare/Dommel.Repositories</PackageProjectUrl>
        <RepositoryUrl>https://github.com/D-Diyare/Dommel.Repositories</RepositoryUrl>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <RepositoryType>git</RepositoryType>
        <Description>Repository Pattern And UnitOfWork Implementation For Dommel.</Description>
        <Copyright>Copyright Â© Diyare Abdulla 2021</Copyright>
        <PackageTags>dommel;crud;dapper;database;orm;repository;repositorypattern;unitofwork;uow</PackageTags>
        <Company>Diyare Abdulla</Company>
        <Authors>Diyare Abdulla</Authors>
        <Title>Dommel.Repositories</Title>
        <PackageLicenseUrl>https://raw.githubusercontent.com/D-Diyare/Dommel.Repositories/master/LICENSE</PackageLicenseUrl>
        <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>
    <ItemGroup>
      <None Remove="nuget.config" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Dommel" Version="2.2.0" />
    </ItemGroup>

</Project>
